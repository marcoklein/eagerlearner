{
  "name": "roguelike",
  "version": "3.22.0",
  "description": "Phaser 3 starter template with TypeScript and webpack.",
  "homepage": "https://github.com/yandeu/phaser-project-template#readme",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack/webpack.dev.js",
    "build": "webpack --config webpack/webpack.prod.js",
    "deploy": "npm run build && gh-pages -d dist",
    "format": "prettier --write src"
  },
  "lint-staged": {
    "src/**/*.ts": "npm run format"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "npm run deploy"
    }
  },
  "keywords": [
    "Phaser",
    "Phaser 3",
    "Phaser3",
    "html5 game",
    "TypeScript",
    "webpack",
    "starter"
  ],
  "author": {
    "name": "Marco Klein",
    "url": "https://github.com/marcoklein"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marcoklein/roguelike.git"
  },
  "template": {
    "name": "phaser-project-template",
    "url": "https://github.com/yandeu/phaser-project-template",
    "author": "Yannick Deubel (https://github.com/yandeu)"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "license": "MIT",
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "gh-pages": "^2.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.4",
    "ts-loader": "^5.4.5",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2",
    "webpack-obfuscator": "^0.27.4",
    "workbox-webpack-plugin": "^5.1.2"
  },
  "dependencies": {
    "phaser": "^3.22.0"
  }
}
